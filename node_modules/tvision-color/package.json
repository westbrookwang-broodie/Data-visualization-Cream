{
  "name": "tvision-color",
  "version": "1.6.0",
  "description": "auto calculate the color for specific scene",
  "homepage": "",
  "license": "ISC",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/esm/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "commitlint:latest": "commitlint --from HEAD~1 --to HEAD --verbose",
    "type-check": "tsc --noEmit",
    "eslint": "eslint --ext .ts --ext .js -c ./.eslintrc.js .",
    "clean": "rimraf ./dist",
    "build": "npm run clean && cross-env NODE_ENV=production npm run build:js",
    "build:js": "rollup -c",
    "build:dev": "npm run clean && cross-env NODE_ENV=development rollup -c",
    "build:watch": "npm run clean && npm run build:type && cross-env NODE_ENV=development rollup -c -w",
    "test": "jest --passWithNoTests",
    "jest": "jest --watch --passWithNoTests",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "7.18.10",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@tencent/eslint-config-tencent": "*",
    "@types/chroma-js": "^2.1.3",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "ts-jest": "^27.1.3",
    "tslib": "2.4.0",
    "typescript": "^4.3.3"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.18.9",
    "@material/material-color-utilities": "0.1.2",
    "bezier-easing": "^2.1.0",
    "chroma-js": "2.1.2"
  },
  "lint-staged": {
    "src/**/*.(t|j)s": "eslint --fix"
  }
}
